{
    "version": "0.2.0",
    "configurations": [
      
      {
        "name": "GeneraRut",
        "program": "${workspaceFolder}/src/generaRut.js",
        "request": "launch",
        "skipFiles": [
          "<node_internals>/**"
        ],
        "type": "pwa-node"
      },
      {
        "name": "Ejecutar electron",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}/src",
        "runtimeExecutable": "${workspaceFolder}/src/node_modules/.bin/electron",
        "args" : ["."],
        "outputCapture": "std"
      }

    ]
  }